END

PASSWORD=$(</flag tail -c +13 | head -c4)
echo "WHAT IS THE PASSWORD???"
read -p "> "

if [ "$REPLY" == "$PASSWORD" ]
then
	echo "CORRECT! Here is your flag:"
	cat /flag
else
	echo "INCORRECT! The right password is '$PASSWORD'."
	echo "This challenge will now self-destruct."
	echo "Restart it to try again."
	rm /flag
	rm -rf /challenge
fi
